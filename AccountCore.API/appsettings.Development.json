{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Testing": {
    "EnableTestEndpoints": true,
    "AllowAnonymousParsing": true
  },
  "Usage": {
    "MonthlyLimit": 100
  },
  "Parser": {
    "DumpDir": "./var/log/parser-dumps",
    "EnableDump": true
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5190"
      },
      "Https": {
        "Url": "https://localhost:7190"
      }
    }
  },
  "HttpsPort": 7190
}